{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"module": "Node16",
		"moduleResolution": "Node16",
		"target": "ES2022",
		"sourceMap": true,
		"outDir": "out",
		"rootDir": "src",
		"lib": [ "ES2023" ],
		"tsBuildInfoFile": "out/compile.tsbuildinfo",
        "incremental": true,
        "composite": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	]
}